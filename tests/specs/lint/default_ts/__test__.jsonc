{
  "tempDir": true,
  "tests": {
    "stdin": {
      "args": "lint -",
      "input": "const x: string = \"foo\";",
      "output": "const x: string = \"foo\";\n"
    },
    "ext_flag": {
      "args": "lint --ext=ts as_ts.js",
      "output": "Checked 1 file\n"
    },
    "extensionless": {
      "args": "lint extensionless",
      "output": "Checked 1 file\n"
    }
  }
}
