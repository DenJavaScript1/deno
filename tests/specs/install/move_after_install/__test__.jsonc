{
  "tempDir": true,
  "envs": {
    "DENO_FUTURE": "1"
  },
  "steps": [
    {
      "cwd": "./test-project",
      "args": "install",
      "output": "[WILDCARD]"
    },
    {
      "cwd": "./test-project",
      "args": "run -A main.mjs",
      "output": "5\n"
    },
    {
      "if": "unix",
      "commandName": "mv",
      "args": "test-project test-project-moved",
      "output": ""
    },
    {
      "if": "unix",
      "cwd": "./test-project-moved",
      "args": "run -A main.mjs",
      "output": "5\n"
    }
  ]
}
