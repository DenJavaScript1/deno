{
  "tests": {
    "info": {
      "args": "info main.ts",
      "output": "info.out"
    },
    "cache": {
      "args": "cache main.ts",
      "output": "cache.out",
      "exitCode": 1
    },
    "check": {
      "args": "check main.ts",
      "output": "check.out",
      "exitCode": 1
    },
    "compile": {
      "args": "compile main.ts",
      "output": "compile.out",
      "exitCode": 1
    },
    "run": {
      "args": "run main.ts",
      "output": "run.out",
      "exitCode": 1
    },
    "builtin_host": {
      "args": "run --quiet builtin_host.ts",
      "output": "Hello World\n"
    },
    "builtin_host_replaced": {
      "args": "run --quiet --allow-import=other.host builtin_host.ts",
      "output": "error: Requires import access[WILDCARD]",
      "exitCode": 1
    }
  }
}
